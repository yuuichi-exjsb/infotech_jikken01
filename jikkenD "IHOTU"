#include<xc.h>
#include<pic.h>
#define _XTAL_FREQ 8000000

#pragma config FOSC = INTOSCIO
#pragma config WDTE = OFF
#pragma config PWRTE = ON
#pragma config MCLRE = OFF
#pragma config BOREN = ON
#pragma config LVP = OFF
#pragma config CPD = OFF
#pragma config WRT = OFF
#pragma config CCPMX = RB3
#pragma config CP = OFF
#pragma config FCMEN = OFF
#pragma config IESO = OFF

void main(void)
{
    OSCCON = 0b01110000;
    
    PORTA = 0x00;
    PORTB = 0x00;
    TRISA = 0x00;
    TRISB = 0x00;

    unsigned char zero = 0b00000000;
    
    unsigned char i1 = 0b10000001;
    unsigned char i2 = 0b11111111;
    
    unsigned char h1 = 0b11111111;
    unsigned char h2 = 0b00010000;
    
    unsigned char O1 = 0b01111110;
    unsigned char O2 = 0b10000001;
    
    unsigned char t1 = 0b10000000;
    unsigned char t2 = 0b11111111;
    
    
    unsigned char u1 = 0b11111111;
    unsigned char u2 = 0b00000001;
    
    //I
    PORTB = i1;
    __delay_ms(5);
    PORTB = i2;
    __delay_ms(5);
    PORTB = i1;
    __delay_ms(5);
    
    PORTB = zero;
    __delay_ms(100);
    
    //H
    PORTB = h1;
    __delay_ms(5);
    PORTB = h2;
    __delay_ms(5);
    PORTB = h2;
    __delay_ms(5);
    PORTB = h2;
    __delay_ms(5);
    PORTB = h1;
    __delay_ms(5);
    
    PORTB = zero;
    __delay_ms(100);
    
    //O
    PORTB = O1;
    __delay_ms(5);
    PORTB = O2;
    __delay_ms(5);
    PORTB = O2;
    __delay_ms(5);
    PORTB = O2;
    __delay_ms(5);
    PORTB =O1;
    __delay_ms(5);
    PORTB = zero;
    __delay_ms(100);
    
    //T
    PORTB = t1;
    __delay_ms(5);
    PORTB = t1;
    __delay_ms(5);
    PORTB = t2;
    __delay_ms(5)
    PORTB = t1;
    __delay_ms(5);
    PORTB = t1;
    __delay_ms(5);
    PORTB = zero
    __delay_ms(100);
    
    //U
    PORTB = u1;
    __delay_ms(5);
    PORTB = u2;
    __delay(5);
    PORTB = u2;
    __delay(5);
    PORTB = u1;
    __delay_ms(5);
    PORTB = zero;
    __delay_ms(100);
    
    
    
    
    
}
